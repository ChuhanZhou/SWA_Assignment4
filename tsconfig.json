{
    "ts-node": {
        "compilerOptions": {
            "module": "commonjs"
        }
    },
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": false,
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "module": "esnext",
        "moduleResolution": "node"
    },
    "exclude": [
        "node_modules",
         "src/**/*.js",
         "src/**/*.jsx",
      ]
}